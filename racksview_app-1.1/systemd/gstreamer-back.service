[Unit]
Description=GStreamer capture from CSI camera
After=network.target

[Service]
ExecStartPre=/bin/sleep 2
ExecStartPre=/usr/bin/rm -f /var/log/racksview/gstreamer-back.log
ExecStart=/opt/racksview/bin/start_gstreamer_back.sh
Restart=on-failure
RestartSec=60
StandardOutput=null
StandardError=file:/var/log/racksview/gstreamer-back.log
MemoryMax=100M

[Install]
WantedBy=multi-user.target
