[Unit]
Description=GStreamer capture from USB camera
After=network.target

[Service]
ExecStartPre=/bin/sleep 2
ExecStartPre=/usr/bin/rm -f /var/log/racksview/gstreamer-front.log
ExecStart=/opt/racksview/bin/start_gstreamer_front.sh
Restart=on-failure
RestartSec=60
StandardOutput=null
StandardError=file:/var/log/racksview/gstreamer-front.log
MemoryMax=256M

[Install]
WantedBy=multi-user.target
